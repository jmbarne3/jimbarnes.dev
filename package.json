  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jmbarne3/jimbarnes.dev/issues"
  },
  "homepage": "https://github.com/jmbarne3/jimbarnes.dev#readme",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-img": "^3.1.0",
    "eleventy-api-indieweb-avatar": "github:11ty/api-opengraph-image",
    "luxon": "^3.3.0",
    "tailwindcss": "^3.3.2"
  },
  "dependencies": {
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
    "eleventy-plugin-seo": "^0.5.2"
  }
